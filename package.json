{
  "name": "word-counter",
  "version": "1.0.0",
  "description": "Counts words in a text file",
  "scripts": {
    "dev": "webpack-dev-server --watch --colors",
    "build": "rimraf dist && NODE_ENV=production webpack --colors",
    "start": "NODE_ENV=production PORT=8080 node ./server.js",
    "test": "mocha --compilers js:babel-core/register --require jsdom-global/register --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint src test webpack",
    "heroku-postbuild": "npm run build"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rkclark/word-counter"
  },
  "author": "Rick Clark",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.8.0",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.16.1",
    "jsdom": "^9.0.0",
    "jsdom-global": "^2.1.1",
    "mocha": "^2.4.5",
    "node-sass": "^3.7.0",
    "react-addons-test-utils": "^15.0.2",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.2.0",
    "sinon": "^2.1.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "express": "^4.13.4",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-dropzone": "^3.12.3",
    "react-router": "^3.0.2",
    "rimraf": "^2.6.1"
  }
